tSize] = useState([window.innerHeight]);\n  useEffect(() => {\n    const handleResize = () => {\n      setSize([window.innerHeight, window.innerWidth]);\n    };\n    window.addEventListener(\"resize\", handleResize);\n  }, []);\n  return size;\n}\n\nfunction MainScreen() {\n  const [height, width] = useWindowSize();\n  const [Project, setProject] = useState(false);\n  const toggleProject = () => setProject(!Project);\n\n  const [Email, setEmail] = useState(false);\n  const toggleEmail = () => setEmail(!Email);\n\n  return (\n    <>\n      <div className=\"App\">\n        <div\n          style={{\n            backgroundImage: `url(${image})`,\n            backgroundSize: \"100vw 100vh\",\n            height: \"100vh\",\n          }}\n        >\n          <div id=\"topBar\">\n            <MainNav>\n              <Switch></Switch>\n            </MainNav>\n          </div>\n          <div id=\"body\">\n            <h3> Welcome </h3>\n            {Project ? <MyProjectWindow toggleProject={toggleProject} /> : null}\n            {Email ? <MyEmailWindow toggleEmail={toggleEmail} /> : null}\n          </div>\n\n          <div id=\"bottomBar\">\n            <Bottom toggleProject={toggleProject} toggleEmail={toggleEmail} />\n          </div>\n        </div>\n      </div>\n\n      {/* <TextAnimation /> */}\n    </>\n  );\n}\n\nconst MainText = styled.div`\n  top: 0%;\n  left: 0%;\n`;\n\nconst EmailBox = styled.div`\n  position: absolute;\n  align-items: flex-start;\n  justify-content: flex-start;\n  top: 30%;\n  left: 15%;\n`;\nexport default MainScreen;\n","/Users/jeffreychan/Desktop/My Website/Personal-Website/src/index.js",[],["100","101"],"/Users/jeffreychan/Desktop/My Website/Personal-Website/src/App.js",[],"/Users/jeffreychan/Desktop/My Website/Personal-Website/src/404.js",[],"/Users/jeffreychan/Desktop/My Website/Personal-Website/src/MainScreen.js",["102"],"import React, { useState, useEffect } from \"react\";\n// import styled from \"styled-components\";\nimport MainNav